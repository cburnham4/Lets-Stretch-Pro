<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="letshangllc.stretchingroutinespro">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/stretch_icon_114"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name="letshangllc.stretchingroutinespro.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="letshangllc.stretchingroutinespro.activities.StretchActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity android:name="letshangllc.stretchingroutinespro.activities.CreateRoutineActivity"></activity>
        <activity android:name="letshangllc.stretchingroutinespro.activities.EditRoutineActivity"></activity>

    </application>

</manifest>